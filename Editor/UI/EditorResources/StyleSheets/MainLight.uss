:root{
    --icon-tint-color: #555555;
}

/************************************************
* General
************************************************/

.horizontal-separator {
    background-color: #7E7E7E;
}

.unity-base-slider__input:focus .unity-base-slider__dragger {
    background-color: #E7E7E7;
}

/************************************************
* Loading Screen
************************************************/
.LoadingScreen {
    background-color: #A5A5A5;
}

/************************************************
* Two Pane SplitView
************************************************/

#unity-dragline-anchor {
    background-color: #7E7E7E;
}

.unity-list-view__item--selected {
    color: white;
}

#categorySelected {
    background-color: #3A79BB;
}

#categorySelected:hover {
    background-color: #3A79BB;
}

.unity-list-view__item--selected {
    background-color: #3A79BB !important;
}

.unity-list-view__item--selected:hover {
    background-color: #3A79BB !important;
}

.unity-list-view__item.unity-list-view__item--selected:hover {
    background-color: #3A79BB !important;
}

.unity-list-view__item.unity-collection-view__item--selected:hover {
    background-color: #3A79BB !important;
}

.unity-list-view__item:hover {
    background-color: #E7E7E7;
}

.collections-top-level-folder > Toggle > VisualElement{
    background-color: #c2c2c2;
}

.unity-foldout__toggle>.unity-toggle__input>.unity-folder__checkmark {
    background-image: resource("uipackageresources/images/in_foldout.png");
}

.unity-foldout__toggle>.unity-toggle__input:focus>.unity-toggle__checkmark {
    background-image: resource("uipackageresources/images/in_foldout.png");
}

.unity-foldout__toggle>.unity-toggle__input:focus:checked>.unity-toggle__checkmark {
    background-image: resource("uipackageresources/images/in_foldout_on.png");
}

.unity-foldout__toggle>.unity-toggle__input:hover:enabled:checked>.unity-toggle__checkmark {
    background-image: resource("uipackageresources/images/in_foldout_on.png");
}

.unity-foldout__toggle>.unity-toggle__input:checked>.unity-toggle__checkmark {
    background-image: resource("uipackageresources/images/in_foldout_on.png");
}

/************************************************
* Tab View
************************************************/
TabView {
    border-color: #7E7E7E;
}

.tab-button--selected .tab-button-label {
    color: black;
}

.tab-button-underscore-bar {
    background-color: #212121;
}

/************************************************
* Search View
************************************************/

.AssetManagerSearchView {
    background-color: #AAAAAA;
    border-color: #AAAAAA;
}

SearchBar {
    background-color: #A5A5A5;
}

#inputSearch {
    background-color: #F0F0F0;
    border-color: #7E7E7E;
}

.SearchFilterChip {
    background-color: #C6C6C6;
}

.SearchFilterChip>Label {
    color: black;
}

/************************************************
* Loading Icon
************************************************/

.loading-bar {
    background-color: white;
}

.unity-top-container{
    background-color: #A5A5A5;
}

.unity-settings-button {
    background-color: #A5A5A5;
}

/************************************************
* Bread Crumbs and Role Container
************************************************/

.unity-breadcrumbs-and-role-container{
    background-color: #A5A5A5;
}

/************************************************
* Bread Crumbs
************************************************/

.unity-breadcrumbs-button:hover {
    color: rgb(50, 50, 50);
}

.unity_breadcrumbs-button:focus {
    color: rgb(50, 50, 50);
}

/************************************************
* Filters
************************************************/

.unity-filters-sort-container {
    background-color: #A5A5A5;
}

.unity-filters {
    background-color: #A5A5A5;
}

.unity-filters-popup {
    background-color: #F0F0F0;
    border-color: #7E7E7E;
}

.unity-filters-chip--set
{
    background-color: #3A79BB;
}

/************************************************
* Sort
************************************************/
.unity-sort-order{
    background-color: #F0F0F0;
}

.unity-sort-order:hover{
    background-color: #E7E7E7;
}

/************************************************
* Grid View
************************************************/

.AssetManagerContentView {
    background-color: #A5A5A5;
}

.grid-view--item {
    background-color: #656565;
}

.grid-view--item-label {
    background-color: #C8C8C8;
}

.grid-view--item-download_cancel_button {
    background-color: white;
}

GridMessageView > .unity-label {
    color: rgb(100, 100, 100);
}

.grid-view--item-ignore .grid-view--item-overlay {
    background-color: rgba(165, 165, 165, 0.85);
}

.grid-view--item-ignore:hover .grid-view--item-overlay {
    background-color: rgba(165, 165, 165, 0.3);
}

.grid-view--item-ignore .grid-view--item-label {
    background-color: rgba(255, 255, 255, 0.5);
    color: rgba(0, 0, 0, 0.6);
}

/************************************************
* Error View
************************************************/

.errorView-action-button-link{
    color: #3A72B0;
    background-color: rgba(0, 0, 0, 0);
}

/************************************************
* Chips
************************************************/

Chip, TagChip, ProjectChip, UserChip  {
    background-color: #C6C6C6;
}

TagChip:hover, ProjectChip:hover, UserChip:hover {
    background-color: #E7E7E7;
}

/************************************************
* Role chip
************************************************/
.unity-role-chip{
    border-color: #585858;
}

.unity-role-chip:hover {
    border-color: #6E6E6E;
    background-color: #E7E7E7;
}

/************************************************
* Multi Selection Page Container
************************************************/

.multi-selection-foldout > Toggle {
    background-color: rgba(198, 198, 198, 1);
}

/************************************************
* Details Page Container
************************************************/

.details-page-thumbnail-container {
    background-color: #A5A5A5;
}

.details-page-tabs Button:focus{
    color: #090909;
    border-bottom-color: #090909;
}

.details-page-tabs > Button{
    color: #090909;
    border-bottom-color: #090909;
}

.details-page-tabs Button.unity-disabled{
    color: #090909;
    border-bottom-color: #090909;
}

.asset-details-page-container {
    background-color: rgba(170, 170, 170, 0.98);
}

.progress-bar {
    background-color: #4C7EFF;
}

.progress-bar-background {
    background-color: #676767;
}

.details-files-foldout > Toggle{
    background-color: rgba(198, 198, 198, 1);
}

.no-files-warning-box {
    background-color: rgba(198, 198, 198, 1);
 }

.no-files-warning-box > Label{
    color: #090909;
}

.same-files-warning-box {
    background-color: rgba(198, 198, 198, 1);
}

.same-files-warning-box > Label{
    color: black;
}

.details-page-close-button:hover, .details-page-download-cancel-button:hover {
    background-color: #EFEFEF;
}

.import-to-button {
    border-color: #B2B2B2;
    border-bottom-color: #939393;
    background-color: #E4E4E4;
}

.import-to-button:hover {
    background-color: #EFEFEF;
}

.import-popup {
    background-color: #F0F0F0;
    border-color: #7E7E7E;
}

.asset-version-details-foldout > Toggle {
    background-color: #C6C6C6;
}

.asset-version-label-latest
{
    background-color: #EFEFEF;
    border-color: #EFEFEF;
}

.asset-version-label-imported
{
    border-color: #EFEFEF;
}

/************************************************
* User Settings
************************************************/

.settings-foldout {
    background-color: #D6D6D6;
    border-color: #8A8A8A;
}

.toolbar-menu,
.asset-manager-dropdown {
    background-color: #E4E4E4;
    border-color: #939393;
}

/************************************************
* Cancellable Progress Bar
************************************************/

.cancellable-progress-bar {
    background-color: #3A3A3A;
}

.cancellable-progress-bar--foreground {
    background-color: #3A79BB;
}

/************************************************
* Custom Section
************************************************/

.upload-page-custom-section,
.in-project-page-custom-section {
    background-color: #C8C8C8;
    border-color: #7E7E7E;
}

.upload-page-settings-panel {
    background-color: rgba(255,255,255,0.5);
}

/************************************************
* Popup
************************************************/

PopupContainer {
    background-color: #C8C8C8;
    border-color: #7E7E7E;
}

/************************************************
* Reimport Window
************************************************/

.reimport-window-footer {
    background-color: #A5A5A5;
}

.reimport-window-warning-container {
    background-color: #A5A5A5;
    border-color: #232323;
}

.reimport-foldout-header-dropdown {
    background-color: #7E7E7E;
    border-color: #232323;
}

.reimport-foldout-content {
    border-color: #232323;
}

/************************************************
* Blocking Progress Panel
************************************************/
.blocking-progress-panel-container {
    background-color: #F0F0F0;
    border-color: #7E7E7E;
}
