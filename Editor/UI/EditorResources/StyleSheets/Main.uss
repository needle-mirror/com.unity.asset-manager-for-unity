    justify-content: center;
    flex-grow: 1;
    margin-top: 24px;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
}

/************************************************
* Timestamp picker
************************************************/

.timestamp-picker-field
{
    width: 49px;
    height: 18px;
}

.timestamp-picker-field .unity-base-popup-field__arrow
{
    margin: 0 4px 0 0;
}

.invalid-field-label
{
    color: red;
}

.invalid-text-field .unity-base-text-field__input
{
    border-color: red;
    border-width: 1px;
    border-radius: 4px;
}

/************************************************
* Multi-selection picker
************************************************/

.multi-selection-picker-entry
{
    flex-direction: row;
    justify-content: space-between;
}

.multi-selection-picker-text
{
    -unity-font-style: bold;
}

.multi-selection-picker-label
{
    -unity-font-style: normal;
}

/************************************************
* Loading icons
************************************************/

.loading-icon {
    align-self: center;
    height: 20px;
    width: 20px;
    flex-shrink: 0;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Loading.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

.loading-bar {
    position: absolute;
    height: 35px;
    padding-left: 20px;
    padding-right: 20px;
    bottom: 10px;
    border-radius: 10px;
    flex-direction: row;
    align-content: center;
    align-items: center;
    align-self: center;
}

.loading-bar--high-position {
    top: 70px;
}

.loading-bar .loading-icon {
    margin-right: 10px;
}

.loading-bar-label {
}

/************************************************
* User Settings
************************************************/

#titleLabel {
    font-size: 19px;
    -unity-font-style: bold;
    padding-top: 0px;
    margin-bottom: 10px;
}

.settings-foldout {
    border-width: 1px;
    margin: 2px 9px 2px 6px;
    min-width: 330px;
}

.settings-foldout Label {
    margin: 2px 0 0 3px;
}

.import-settings-container {
    margin-bottom: 20px;
    margin-top: 10px;
}

.cache-settings-container {
    flex-direction: row;
    margin-bottom: 20px;
    margin-top: 10px;
}

#cacheSizeOnDiskContainer {
    flex-direction: row;
    align-self: flex-start;
    margin-top: 5px;
}

.cache-settings-label,
.cache-settings-element {
    padding-left: 10px;
}

.cache-settings-element {
    flex-grow: 1;
}

.cache-settings-label {
    padding-bottom: 9px;
}

#cacheSize {
    padding-top: 5px;
}

#unity-text-field {
    max-width: 50px;
}

#cacheSizeOnDisk {
    padding-top: 1px;
    flex-grow: 1;
}

.upload-settings-container {
    margin-bottom: 20px;
    margin-top: 10px;
}

.upload-settings-label {
    width: 400px;
}

.confidence-threshold-container {
    flex-direction: row;
    margin-top: 5px;
}

.confidence-threshold{
    flex-grow: 1;
}

.asset-manager-path {
    flex-grow: 1;
    flex-shrink: 1;
    text-overflow: ellipsis;
    overflow: hidden;
    border-radius: 3px;
    border-width: 1px;
}

.toolbar-menu {
    border-width: 1px;
    margin-right: 5px;
    height: 24px;
    flex-direction: row;
}

.folder-image {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Common/Folder-Opened.png');
    width: 20px;
}

.asset-manager-dropdown {
    border-width: 0;
}

.max-cache-size {
    margin: 2px 5px;
}

#cleanCache {
    width: 120px;
    margin-left: 10px;
}

.label-toolbar-combo {
    flex-direction: row;
    flex-grow: 1;
    justify-content: space-between;
}

.import-setting-name {
    width: 250px;
    padding-bottom: 7px;
}

/************************************************
* Cancellable Progress Bar
************************************************/

.cancellable-progress-bar {
    position: absolute;
    flex-grow: 1;
    height: 15px;
}

.cancellable-progress-bar--foreground {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
}

.cancellable-progress-bar--cancel-button {
    position: absolute;
    width: 16px;
    height: 16px;
    top: 0;
    bottom: 0;
    right: -16px;
    padding: 0;
    margin: 0;
    border-radius: 0;
}

.align-with-top {
    left: 0;
    right: 16px;
    top: 0;
}

/************************************************
* Icons
************************************************/

.default-asset-icon {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Common/Default-Asset.png');
}

.status-icon-account {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Common/Icon-Account.png');
}

.icon-all-assets {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/All-Assets.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
    width: 16px;
    height: 16px;
}

.icon-in-project {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/In-Project.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
    width: 16px;
    height: 16px;
}

.icon-default-project {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Project-Icon.png');
    background-size: 12px 12px;
    background-position: center;
}

/************************************************
* Upload
************************************************/
.upload-page-settings-panel {
    flex-direction: column;
    position: absolute;
    bottom: 40px;
    right: 0;
    padding: 5px;
    margin-bottom: 5px;
    margin-right: 10px;
    border-radius: 8px;
}

.upload-page-settings-panel DropdownField Label.unity-text-element.unity-label.unity-base-field__label.unity-base-popup-field__label.unity-popup-field__label{
    min-width: 0;
    flex-shrink: 1;
    flex-grow: 1;
    text-overflow: ellipsis;
    overflow: hidden;
}

.upload-page-settings-panel DropdownField .unity-popup-field__input {
    flex-grow: 0;
    flex-shrink: 0;
    min-width: 140px;
}

.upload-page-all-settings-section {
    flex-direction: row;
    -unity-text-align: middle-left;
}

.upload-page-settings-project-section {
    margin-right: 20px;
}

/************************************************
* Custom Section
************************************************/

.upload-page-custom-section,
.in-project-page-custom-section {
    flex-direction: row;
    justify-content: flex-end;
    padding: 5px;
    border-top-width: 1px;
}

.upload-page-action-section,
.in-project-page-action-section {
    flex-direction: row;
}

.upload-page-all-actions-section,
.in-project-page-all-actions-section{
    flex-direction: row;
    height : 30px;
    align-self: center;
}

.upload-page-upload-button,
.upload-page-reset-button,
.in-project-page-update-all-button{
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
}

/************************************************
* Popup
************************************************/

PopupContainer {
    flex-direction: column;
    position: absolute;
    padding: 5px;
    margin-top: 5px;
    border-width: 1px;
    border-radius: 8px;
}

/************************************************
* Reimport Window
************************************************/
.reimport-window-content {
    flex-grow: 1;
    margin: 10px;
}

.reimport-window-footer {
    height: 32px;
    padding: 4px;
    flex-direction: row;
    justify-content: flex-end;
    flex-shrink: 0;
}

.reimport-window-conflicts-title {
    white-space: normal;
}

.reimport-window-warning-container {
    flex-direction: row;
    margin-top: 10px;
    border-width: 1px;
    align-items: center;
}

.reimport-window-dependents-title {
    white-space: normal;
}

.reimport-window-warning-icon {
    width: 30px;
    height: 30px;
    flex-shrink: 0;
    margin: 4px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Common/Warning.png');
}

.reimport-window-gap {
    height: 16px;
    display: none;
}

.reimport-foldout {
    margin-top: 8px;
}

.reimport-foldout Toggle {
    margin-left: 0;
    margin-right: 0;
    flex-shrink: 1;
}

.reimport-foldout Toggle .unity-toggle__input {
    flex-shrink: 1;
}

.reimport-foldout-header
{
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    flex-grow: 1;
}

.reimport-foldout-header-text
{
    margin-left: 4px;
    flex-direction: row;
}

.reimport-foldout-header-text-name
{
    -unity-font-style: bold;
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
}

.reimport-foldout-header-dropdown
{
    flex-direction: column;
    border-radius: 4px;
    margin-right: 0;
    flex-shrink: 0;
    flex-grow: 0;
    width: 85px;
}

.reimport-foldout-header-dropdown .unity-base-popup-field__text
{
    margin: 0;
}

.reimport-foldout-content
{
    margin-left: 6px;
    border-width: 1px;
    flex-direction: column;
}

.reimport-foldout-content-entry {
    flex-direction: row;
    margin-top: 4px;
    margin-bottom: 4px;
    margin-left: 8px;
    margin-right: 16px;
    justify-content: space-between;
    align-items: center;
}

.reimport-foldout-content-entry-name {
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
}

.reimport-foldout-content-entry-status--warning
{
    flex-shrink: 0;
    width: 16px;
    height: 16px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Common/Warning.png');
}

.upward-dependency-item-asset-name {
    margin-top: 8px;
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
    -unity-font-style: bold;
}

/************************************************
* Blocking Progress Panel
************************************************/
BlockingProgressPanel {
	width: 100%;
    height: 100%;
	position: absolute;
	background-color: rgba(0,0,0,0);
	flex-direction: column;
	justify-content: center;
	align-items: center;
}

BlockingProgressPanel Label {
    font-size: 14px;
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
    margin-top: 10px;
}

BlockingProgressPanel ProgressBar {
    height: 6px;
    align-self: stretch;
    align-items: stretch;
}

BlockingProgressPanel ProgressBar .progress-bar-container {
    border-radius: 3px;
    flex-grow: 1;
}

BlockingProgressPanel ProgressBar .progress-bar {
    border-radius: 3px;
}

.blocking-progress-panel-container {
    width: 30%;
    justify-content: center;
    align-items: center;
    border-radius: 8px;
    border-width: 1px;
    padding: 12px;
    margin: 32px;
}
