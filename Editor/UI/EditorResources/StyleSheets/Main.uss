/************************************************
* General
************************************************/

.horizontal-separator {
    height: 1px;
    flex-shrink: 0;
}

.flex-shrink-0 {
    flex-shrink: 0;
}

.flex-shrink-1 {
    flex-shrink: 1;
}

.flex-grow-1 {
    flex-grow: 1;
}

.incomplete-file-icon {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Warning.png');
    -unity-background-image-tint-color: #FF554A;
}

.flex-row {
    flex-direction: row;
}

.flex-column {
    flex-direction: column;
}

/************************************************
* Loading Screen
************************************************/
.LoadingScreen {
    height: 100%;
    width: 100%;
    align-items: center;
    justify-content: center;
}

.LoadingScreen Label {
    font-size: 14px;
    -unity-font-style: bold;
    margin-top: 12px;
}

/************************************************
* Two Pane SplitView / Categories
************************************************/

.collapsed-sidebar {
    min-width: 32px;
    max-width: 32px;
    border-right-width: 1px;
    border-right-color: black;
}

.expanded-sidebar {
    min-width: 160px;
    max-width: none;
}

.collapse-sidebar-button {
    height: 32px;
    width: 32px;
    justify-content: center;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Left-Arrow.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
    background-size: 24px 24px;
    background-position-x: left;
}

.collapse-sidebar-button:focus{
    border-width: 1px;
    border-color: #7BAEFA;
}

.expand-sidebar-button {
    height: 32px;
    width: 32px;
    justify-content: center;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Right-Arrow.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
    background-size: 24px 24px;
}

.expand-sidebar-button:focus{
    border-width: 1px;
    border-color: #7BAEFA;
}

.sidebar-top-section {
    flex-direction: row;
    justify-content: space-between;
    min-height: 32px;
    max-height: 32px;
}

#FooterContainer {
    justify-content: flex-end;
    margin-top: 8px;
    flex-grow: 1;
    flex-shrink: 0;
}

.SidebarTitle{
    margin: 8px 0 0 8px;
    -unity-font-style: bold;
}

SideBarFoldout .unity-base-field__input{
    flex-shrink: 1;
}

SideBarFoldout Image {
    max-height: 16px;
    max-width: 16px;
    align-self: center;
    flex-shrink: 0;
}

SideBarFoldout ToolbarSpacer {
    flex-shrink: 0;
}

SideBarFoldout Label {
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
}

SideBarFoldout Toggle {
    margin-left: 0;
    margin-right: 0;
}

SideBarAllAssetsFoldout Image {
    width: 16px;
    height: 16px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/All-Assets.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

SideBarCollectionFoldout Image.icon-folder-open {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Folder-Open.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
    width: 16px;
    height: 16px;
}

SideBarCollectionFoldout Image.icon-folder-close {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Folder-Close.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
    width: 16px;
    height: 16px;
}

#categoryFoldoutButton {
    height: 100%;
    width: 30px;
}

SideBarButton>ListView {
    margin-left: 5px;
}

SideBarButton {
    flex-direction: row;
    min-height: 32px;
    width: auto;
    border-width: 1px;
    border-color: rgba(0,0,0,0);
}

SideBarButton:focus{
    border-width: 1px;
    border-color: #7BAEFA;
}

SideBarButton>Image {
    max-height: 16px;
    max-width: 16px;
    align-self: center;
}

SideBarButton>Label {
    align-self: center;
}

.SideBarContainer {
    justify-content: space-between;
    flex-basis: auto;
}

.SelectionInspectorContainer{
    min-width: 200px;
    flex-basis: auto;
}

.ContentPanel{
    flex-basis: auto;
    flex-direction: row;
    flex-grow: 1;
    width: 100%;
}

TwoPaneSplitView {
    flex: 1 0 0;
}

#unity-dragline-anchor {
    width: 1px;
}

.ProjectsDropdown {
    height: 30px;
    padding-top: 10px;
    margin-bottom: 10px;
    margin-left: 5px;
}

.unity-base-popup-field__text {
    text-overflow: ellipsis;
}

.unity-base-popup-field__arrow {
    margin-left: 10px;
}

#categories-scrollView {
    padding-top: 10px;
}

.NoProjectsWarning {
    flex-direction: row;
    align-items: center;
    margin: 1px 3px 10px 10px;
    padding-top: 10px
}

.NoProjectSelected {
    -unity-font-style: italic;
    margin-left: 10px;
    margin-top: 5px;
}

/*!* Overrides the blue triangle in foldout*!*/
.unity-foldout__toggle>.unity-toggle__input:focus:checked>.unity-toggle__checkmark {
    background-image: resource("uipackageresources/images/d_in_foldout_on.png");
}

.unity-foldout__toggle>.unity-toggle__input:focus>.unity-toggle__checkmark {
    background-image: resource("uipackageresources/images/d_in_foldout.png");
}

.unity-foldout {
    margin-left: 5px;
}

.unity-foldout__toggle {
    margin-left: 0;
}

.unity-collection-view__item {
    margin-left: 5px;
}

.removed-arrow {
    margin-left: 22px;
}

.allAssetsFolder Toggle{
    padding-left: 0px;
}

.allAssetsFolder {
    margin-left: 5px;
}

.collections-top-level-folder > Toggle{
    padding-left: 4px;
}

.collections-top-level-folder > Toggle > VisualElement > Image{
    display: none;
    width: 0px;
}

.collections-top-level-folder > Toggle > VisualElement > ToolbarSpacer{
    width: 3px;
}

.collections-top-level-folder > Toggle > VisualElement > Label{
    -unity-font-style: bold;
}

.SearchBar__placeholder {
    -unity-font-style: italic;
    opacity: 0.5;
}

/*This is a UIToolkit bug where the second element of the foldouts' list
is not intended properly and we need this to fix that issue*/
.unity-foldout--depth-1 .unity-foldout__toggle,
.unity-foldout--depth-2 .unity-foldout__toggle,
.unity-foldout--depth-3 .unity-foldout__toggle,
.unity-foldout--depth-4 .unity-foldout__toggle,
.unity-foldout--depth-max .unity-foldout__toggle {
    margin-left: 0;
}

/************************************************
* Top Container
************************************************/
.unity-top-container {
    flex-direction: row;
    flex-grow: 1;
    flex-shrink: 0;
}

/************************************************
* Top Left Container
************************************************/
.unity-top-left-container {
    flex-direction: row;
    justify-content: flex-start;
    flex-grow: 1;
    flex-shrink: 1;
    padding-top: 8px;
    padding-right: 12px;
    flex-wrap: wrap;
}

/************************************************
* Top Right Container
************************************************/
.unity-top-right-container {
    flex-direction: row;
    justify-content: flex-end;
    flex-grow: 1;
    flex-shrink: 1;
}

/************************************************
* Bread Crumbs
************************************************/

.unity-breadcrumbs {
    flex-direction: row;
    align-self: center;
    padding-left: 6px;
    flex-wrap: wrap;
    flex-shrink: 0;
}

.unity-breadcrumbs-arrow {
    width: 16px;
    min-width: 16px;
    height: 16px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Caret-Right.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

.unity-breadcrumbs-button {
    height: 16px;
    padding-left: 8px;
    padding-right: 8px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    -unity-text-overflow-position: end;
}

.unity-breadcrumbs-button.highlight {
    -unity-font-style: bold;
}

/************************************************
* Settings Button
************************************************/
.unity-settings-button {
    height: 20px;
    width: 20px;
    flex-shrink: 0;
    border-width: 0;
    border-radius: 0;
    margin: 0;
    padding: 0;
    margin-top: 8px;
    margin-right: 8px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Gear.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}


/************************************************
* Filters
************************************************/
.unity-filters-sort-container{
    flex-direction: row;
    flex-wrap: wrap;
    flex-shrink: 0;
    flex-grow: 1;
    justify-content: space-between;
}

.unity-filters {
    flex-direction: row;
    flex-wrap: wrap;
    flex-shrink: 0;
    padding-top: 6px;
    padding-left: 8px;
    padding-bottom: 6px;
}

.unity-filters-button {
    flex-direction: row;
    flex-wrap: nowrap;
    flex-shrink: 0;
    flex-grow: 0;
    justify-content: flex-start;
    align-items: center;
    border-width: 1px;
    margin: 0;
    margin-bottom: 5px;
    padding-top: 3px;
    padding-bottom: 3px;
}

.unity-filters-button-caret {
    width: 12px;
    height: 12px;
    margin-left: 12px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Caret-Down.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

.unity-filters-popup{
    flex-direction: column;
    position: absolute;
    padding: 5px;
    margin-top: 5px;
    min-width: 160px;
    border-width: 1px;
    border-radius: 8px;
}

.unity-filters-popup-filter-selection {
    flex-direction: row;
    font-size: 14px;
    padding-top: 3px;
    padding-bottom: 3px;
    padding-right: 8px;
    padding-left: 8px;
    border-radius: 5px;
}

.unity-filters-popup-filter-selection:hover {
    background-color: #2F60C1;
}

.unity-filters-popup-filter-selection > Image {
    margin-left: 0;
    margin-right: 4px;
    width: 16px;
    height: 16px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Check.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

.unity-filters-popup-filter-no-selection {
    font-size: 14px;
    padding: 3px 8px;
}

.unity-filters-chip-container {
    flex-direction: row;
    flex-wrap: wrap;
    flex-shrink: 0;
}

.unity-filters-chip {
    flex-direction: row;
    border-radius: 12px;
    flex-shrink: 0;
    align-items: center;
    margin-bottom: 5px;
}

.unity-filters-chip-delete {
    margin-left: 8px;
    margin-right: 4px;
    width: 12px;
    height: 12px;
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Close.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

/************************************************
* Sort
************************************************/
.unity-sort{
    flex-direction: row;
    flex-shrink: 0;
    padding-top: 6px;
    padding-right: 8px;
    padding-bottom: 6px;
    margin-bottom: 3px;
}

.unity-sort-dropdown > .unity-label {
    min-width: 0;
    align-self: center;
}

.unity-sort-dropdown > .unity-popup-field__input {
    width: 110px;
}

.unity-sort-order {
    border-width: 0;
    margin: 0;
    padding: 0;
    width: 21px;
    height: 21px;
    align-self: center;
}

.unity-sort-order:focus {
    border-width: 1px;
}

.unity-sort-order--ascending {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Arrow-Up.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

.unity-sort-order--descending {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Arrow-Down.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

/************************************************
* NOT LOGGED IN VIEW
************************************************/

.header-container {
    flex-direction: row;
    border-bottom-width: 1px;
    border-bottom-color: rgba(0, 0, 0, 0.2);
    background-color: rgba(255, 255, 255, 0.2);
}

.header-thumbnail {
    width: 64px;
    height: 64px;
    -unity-background-scale-mode: scale-to-fit;
    margin-left: 14px;
    margin-right: 14px;
    margin-top: 14px;
    margin-bottom: 14px;
}

.header-project-info {
    margin-top: 14px;
}

.header-project-name {
    font-size: 14px;
    -unity-font-style: bold;
    margin-top: 12px;
}

.header-user-email {
    font-size: 14px;
    margin-top: 4px;
}

.main-container {
    align-items: center;
    flex-grow: 1;
    justify-content: center;
}

.status-icon {
    width: 100px;
    height: 100px;
    -unity-background-scale-mode: scale-to-fit;
}

.progress-bar {
    width: 200px;
}

.progress-label {
    -unity-font-style: bold;
    font-size: 14px;
    margin-top: 15px;
    margin-left: 30px;
    margin-right: 30px;
    -unity-text-align: upper-center;
    white-space: normal;
}

.bottom-button {
    height: 28px;
    margin: 15px;
    font-size: 13px;
    -unity-font-style: bold;
}

.info-text {
    margin-top: 5px;
    white-space: normal;
    -unity-text-align: upper-center;
    margin-left: 30px;
    margin-right: 30px;
    margin-bottom: 15px;
    font-size: 13px;
    max-width: 400px;
}

.main-container-light {
    background-color: rgba(0, 0, 0, 0.1);
    border-bottom-width: 1px;
    border-bottom-color: rgba(0, 0, 0, 0.1);
}

.bottom-button-blue {
    background-color: rgb(33, 150, 243);
    color: rgb(255, 255, 255);
    border-left-color: rgba(0, 0, 0, 0.1);
    border-right-color: rgba(0, 0, 0, 0.1);
    border-top-color: rgba(0, 0, 0, 0.1);
    border-bottom-color: rgba(0, 0, 0, 0.2);
}

.bottom-button-blue:hover {
    background-color: rgb(11, 131, 228);
}

.link {
    color: rgb(61, 119, 194);
    -unity-font-style: bold;
    white-space: normal;
}

/************************************************
* Tab View
************************************************/
TabView {
    flex-direction: row;
    justify-content: flex-start;
    min-height: 32px;
    flex-shrink: 0;
    padding-left: 5px;
    flex-wrap: wrap;
    border-bottom-width: 1px;
}

.tab-button {
    justify-content: center;
}

.tab-button-label {
    font-size: 12px;
    -unity-text-align: middle-center;
    margin-left: 20px;
    margin-right: 20px;
}

.tab-button-underscore-bar {
    display: none;
    background-color: white;
    height: 2px;
    flex-grow: 1;
    position: absolute;
    bottom: 2px;
    right: 0;
    left: 0;
}

.tab-button--selected .tab-button-underscore-bar{
    display: flex;
}

/************************************************
* Search View
************************************************/

.AssetManagerSearchView {
    width: 100%;
    height: 70px;
}

SearchBar {
    height: 42px;
    flex-shrink: 0;
}

.page-title-label {
    align-self: flex-start;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    margin-left: 10px;
    height: 30px;
}

#inputSearch {
    height: 30px;
    width: auto;
    min-width: 0;
    justify-content: flex-start;
    align-items: center;
    flex-wrap: nowrap;
    border-radius: 4px;
    margin: 8px;
    border-width: 1px;
}

#unity-search {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Search.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

#search-clear {
    background-image: resource('Packages/com.unity.asset-manager-for-unity/Editor/UI/EditorResources/Images/Icons/Close.png');
    -unity-background-image-tint-color: var(--icon-tint-color);
}

.SearchContentSplitView {
    min-width: 130px; /* Roughly the width of a grid item */
    flex-grow: 1;
}

/* The search icon in the search bar*/
#unity-search {
    height: 16px;
    width: 16px;
    margin: 0 5px 0 5px;
}

#refresh-button-icon {
    height: 20px;
    width: 20px;
    cursor: link;
}

TextField {
    height: 100%;
    width: 100%;
}

#unity-text-input {
    height: auto;
    margin-left: 5px;
}

.search-chip-container {
    justify-content: space-between;
    flex-direction: row;
}

.SearchFilterChip {
    overflow: hidden;
    text-overflow: ellipsis;
    justify-content: space-around;
    width: auto;
    flex-shrink: 1;
    max-width: 150px;
    border-radius: 8px;
    padding: 1px 4px 1px 8px;
    margin-left: 6px;
    height: 18px;
}

.SearchFilterChip>Label {
    overflow: hidden;
    text-overflow: ellipsis;
    width: auto;
    padding-right: 5px;
}

.SearchFilterChip>Button {
    flex-shrink: 0;
}

#search-clear {
    visibility: hidden;
    margin-right: 5px;
    height: 10px;
    width: 10px;
    flex-shrink: 0;
}

/************************************************
* Chips
************************************************/

Chip, TagChip {
    padding: 5px 10px;
    padding-right: 10px;
    border-radius: 12px;
    margin: 2px;
    flex-shrink: 0;
}

TagChip {
    cursor: link;
}

ProjectChip, UserChip {
    padding: 5px 5px;
    padding-right: 10px;
    border-radius: 12px;
    margin-right: 5px;
    flex-shrink: 1;
    flex-direction: row;
    cursor: link;
}

RoleChip {
    flex-shrink: 0;
    flex-grow: 0;
    align-self: center;
    margin-left: 8px;
    border-width: 1px;
    border-radius: 10px;
    padding: 1px 4px;
    cursor: link;
}

Chip > Label, TagChip > Label, ProjectChip > Label, UserChip > Label {
    text-overflow: ellipsis;
    overflow: hidden;
    flex-shrink: 1;
    -unity-text-overflow-position: end;
}

RoleChip > Label {
    -unity-text-align: middle-center;
}

ProjectChip > Image
{
    width: 16px;
    height: 16px;
    padding: 2px;
    border-radius: 8px;
    background-color: blueviolet;
    margin-right: 5px;
    flex-shrink: 0;
}

UserChip #InitialsCircle
{
    width: 16px;
    height: 16px;
    padding: 2px;
    border-radius: 8px;
    margin-right: 5px;
    flex-shrink: 0;
}

UserChip #Initials
{
    -unity-text-align: middle-center;
    -unity-font-style: bold;
    color: white;
    font-size: 8px;
    flex-shrink: 0;
    padding: 0;
    padding-top: 1px;
}

/************************************************
* Grid View
************************************************/

.AssetManagerContentView {
    height: 100%;
}

AssetsGridView {
    flex-grow: 1;
}

.grid-view {
    margin-bottom: 2px;
    padding-top: 2px;
    flex-grow: 1;
    min-width: 100%;
}

.grid-view--item {
    flex-basis: auto;
    flex-direction: column;
    justify-content: center;
    flex-grow: 0;
    flex-shrink: 0;
    height: 150px;
    margin: 3px;
    width: 125px;
    border-width: 2px;
    border-radius: 5px;
    border-color: rgba(0, 0, 0, 0);
}

.grid-view--item .loading-icon {
    position: absolute;
}

.asset-preview-thumbnail {
    align-self: center;
    flex-basis: auto;
    flex-grow: 1;
    flex-shrink: 0;
    height: 100%;
    width: 100%;
    -unity-background-scale-mode: scale-to-fit;
}

.grid-view--item-label {
    text-overflow: ellipsis;
    overflow: hidden;
    bottom: 0px;
    width: 100%;
    height: 25px;
    visibility: visible;
    font-size: 11px;
    padding-left: 6px;
    -unity-text-align: middle-left;
    border-bottom-left-radius: 3px;
    border-bottom-right-radius: 3px;
}

.grid-view--item-selected {
    border-color: rgb(127, 214, 252); /* Similar blue to Prefabs icons in the Editor */
}

.grid-view--item:hover {
    border-color: rgba(255, 255, 255, 0.3);
}

.grid-view--item-selected:hover {
    border-color: rgb(127, 214, 252); /* Similar blue to Prefabs icons in the Editor */
}

.grid-view--item-overlay {
    display: none;
}

.grid-view--item:hover .grid-view--item-overlay {
    display: flex;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(255, 255, 255, 0.1);
}

.grid-view--item-ignore .grid-view--item-overlay {
    display: flex;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.grid-view--item-ignore:hover .grid-view--item-overlay {
    display: flex;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.grid-view--item:hover .asset-preview--upload .asset-preview-asset-type-icon{
    -unity-background-image-tint-color: rgba(255, 255, 255, 0);
}

.grid-view--item:hover .asset-preview--upload.no-thumbnail .asset-preview-asset-type-icon {
    -unity-background-image-tint-color: rgba(255, 255, 255, 255);
}

.grid-view--item:hover .asset-preview--upload .asset-preview-toggle {
    display: flex;
}

.grid-view--item-dummy {
    border-color: rgba(255, 255, 255, 0.5);
    border-radius: 2px;
    cursor: arrow;
    flex-basis: auto;
    flex-grow: 0;
    flex-shrink: 0;
    height: 150px;
    margin: 2.5px;
    width: 125px;
}

.grid-view--item-download_progress_bar > * {
    height: 3px;
    border-bottom-left-radius: 2px;
    border-bottom-right-radius: 2px;
}

.grid-view--item-download_cancel_button {
