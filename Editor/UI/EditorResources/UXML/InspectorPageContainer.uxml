                <ui:Label name="inspector-page-title" class="text-bold flex-shrink-1 flex-grow-1" />
            </ui:VisualElement>
            <ui:Button tabindex="-1" display-tooltip-when-elided="true" name="closeButton" class="details-page-close-button" style="margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
