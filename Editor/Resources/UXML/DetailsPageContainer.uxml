<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" ae="Unity.AssetManager.Editor" editor-extension-mode="True" class="px-2">
    <ui:VisualElement name="assetDetailsPageContainer" class="asset-details-page-container justify-space-between">
        <ui:VisualElement name="details-page-top" class="details-page-top">
            <ui:VisualElement name="details-page-asset-name-container" class="details-page-asset-name-container">
                <ui:Label name="asset-name" class="text-bold" />
                <ui:Image name="asset-dashboard-link" />
            </ui:VisualElement>
            <ui:Button tabindex="-1" display-tooltip-when-elided="true" name="closeButton" class="details-page-close-button" style="margin-left: 0; margin-right: 0; margin-top: 0; margin-bottom: 0;" />
        </ui:VisualElement>
        <ui:ScrollView name="details-page-scrollview" mode="Vertical" class="details-page-scrollview">
            <ui:VisualElement name="details-page-content-container">
                <ui:VisualElement name="details-page-entries-container" class="details-page-entries-container" >
                    <ui:Label name="description" class="details-page-entry whitespace-wrap" />
                    <ui:VisualElement name="details-page-thumbnail-container" class="details-page-entry image-container self-center" />
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Status" class="details-page-entry-label" />
                        <ui:Label name="status" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement name="details-page-project-container" class="details-page-entry">
                        <ui:Label text="Project" class="details-page-entry-label" />
                        <ui:VisualElement name="details-page-project-pill-container" />
                    </ui:VisualElement>
                    <ui:VisualElement name="details-page-tag-section" class="details-page-entry">
                        <ui:Label text="Tags" class="details-page-entry-label" />
                        <ui:VisualElement name="details-page-tags-container" class="flex-row flex-wrap flex-grow-1" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Total Files" class="details-page-entry-label" />
                        <ui:Label name="total-files" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="File Size" class="details-page-entry-label" />
                        <ui:Label name="filesize" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Asset ID" class="details-page-entry-label" />
                        <ui:Label name="id" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Asset Type" class="details-page-entry-label" />
                        <ui:Label name="assetType" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Upload Date" class="details-page-entry-label" />
                        <ui:Label name="upload-date" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Last Edited" class="details-page-entry-label" />
                        <ui:Label name="last-edited" class="details-page-entry-value" />
                    </ui:VisualElement>
                    <ui:VisualElement class="details-page-entry">
                        <ui:Label text="Version" class="details-page-entry-label" />
                        <ui:Label name="version" class="details-page-entry-value" />
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:VisualElement name="no-files-warning-box">
                    <ui:Image name="no-files-warning-icon" />
                    <ui:Label name="no-files-warning-label" class="text-bold" />
                </ui:VisualElement>
                <ui:VisualElement name="details-files-foldout-container" class="flex-row">
                    <ui:Foldout text="Included Files" name="details-files-foldout" class="details-files-foldout">
                        <ui:Label name="details-files-loading-label"/>
                        <ui:ListView name="details-files-listview" />
                    </ui:Foldout>
                </ui:VisualElement>
            </ui:VisualElement>
        </ui:ScrollView>
        <ui:VisualElement name="footer" class="details-page-footer px-2">
            <ui:VisualElement name="footer-container" class="details-page-footer-container">
                <ui:Button name="removeImportButton" class="big-button" />
                <ui:Button name="showInProjectBrowserButton" class="big-button" />
                <ui:Button name="importButton" class="big-button" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
